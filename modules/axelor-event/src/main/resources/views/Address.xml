<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

  <grid model="com.axelor.event.db.Address" title="Address" name="address-grid">
    <field name="flatHousrNo"></field>
    <field name="street"></field>
    <field name="landMark"></field>
    <field name="city"></field>
    <field name="country"></field>
  </grid>

  <form model="com.axelor.event.db.Address" title="Address" name="address-form">
    <panel title="Address">

      <field name="flatHousrNo"></field>
      <field name="street"></field>
      <field name="landMark"></field>
      <field name="city"></field>
      <field name="country"></field>
      <field name="fullName" colSpan="5"></field>
    </panel>
  </form>

</object-views>
